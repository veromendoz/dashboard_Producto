<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="315.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.fx10.controlador.ListaProductoController">
    <children>
        <HBox layoutX="11.0" layoutY="14.0" prefHeight="31.0" prefWidth="579.0">
            <children>
                <Label text="Lista De Producto" />
            </children>
        </HBox>
        <Separator layoutX="21.0" layoutY="47.0" prefHeight="11.0" prefWidth="560.0" />
        <TableView fx:id="tablaProductos" layoutX="30.0" layoutY="62.0" prefHeight="200.0">
            <columns>
                <TableColumn fx:id="colCodigo" prefWidth="84.0" text="Codigo" />
                <TableColumn fx:id="colNombre" prefWidth="116.0" text="Nombre" />
                <TableColumn fx:id="colDescripcion" prefWidth="207.0" text="Descripcion" />
                <TableColumn fx:id="colPrecio" minWidth="6.0" prefWidth="68.0" text="Precio" />
                <TableColumn fx:id="colStock" minWidth="9.0" prefWidth="65.0" text="Stock" />
            </columns>
        </TableView>
        <Button fx:id="btnEliminar" layoutX="519.0" layoutY="276.0" mnemonicParsing="false" onAction="#onEliminar" text="Eliminar" />
        <Button fx:id="btnCancelar" layoutX="450.0" layoutY="276.0" mnemonicParsing="false" onAction="#onCancelar" text="Cancelar" />
    </children>
</AnchorPane>